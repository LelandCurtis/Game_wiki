<%= form_with url: '/roles', method: :PATCH, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, :id => 'name' %>
  <%= form.label :unlocked %>
  <%= form.select :unlocked, [['False',false], ['True', true]], :id => 'unlocked' %>
  <%= form.label :health %>
  <%= form.number_field :health, :id => 'health' %>
  <%= form.button :Update%>
<% end %>
