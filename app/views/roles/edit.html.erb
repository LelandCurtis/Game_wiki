<%= form_with url: "/roles/#{@role.id}", method: :patch, local: true do |form|  %>
  <%= form.label :name %>
  <%= form.text_field :name, :id => 'name'%><br>
  <%= form.label :unlocked %>
  <%= form.select :unlocked, [["False", false], ["True", true]], :id => 'unlocked' %><br>
  <%= form.label :health %>
  <%= form.number_field :health, :id => 'health' %><br>
  <%= form.submit "Update Role" %><br>
<%= end %>
