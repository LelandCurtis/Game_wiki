<%= form_with url: '/roles', method: :post, local: true do |form|%>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :unlocked %>
  <%= form.select :unlocked, [false, true] %>
  <%= form.submit "Create Role" %>
<% end %>
